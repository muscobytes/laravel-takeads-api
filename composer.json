{
    "name": "muscobytes/laravel-takeads-api-client",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "muscobytes/takeads-api-client": "^1.0",
        "php-http/discovery": "1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "11.4.x-dev",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Muscobytes\\Laravel\\TakeadsApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muscobytes\\Laravel\\TakeadsApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muscobytes\\Laravel\\TakeadsApi\\ClientServiceProvider"
            ],
            "aliases": {
                "TakeadsApi": "Muscobytes\\Laravel\\TakeadsApi\\Facades\\TakeadsApi"
            }
        }
    }
}
